{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^1.2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/autoembed",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/autoembed/releases/autoembed_4.13.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/autolink",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/autolink/releases/autolink_4.13.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/embed",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/embed/releases/embed_4.13.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/embedbase",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/embedbase/releases/embedbase_4.13.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/embedsemantic",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/embedsemantic/releases/embedsemantic_4.13.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/notification",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/notification/releases/notification_4.13.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/notificationaggregator",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/notificationaggregator/releases/notificationaggregator_4.13.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/textmatch",
                "version": "4.13.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/textmatch/releases/textmatch_4.13.0.zip",
                    "type": "zip"
                }
            }
        },

        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/wordcount",
                "version": "1.17.6",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/wordcount/releases/wordcount_1.17.6.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "ckeditor-plugin/autoembed": "4.13.0",
        "ckeditor-plugin/autolink": "4.13.0",
        "ckeditor-plugin/embed": "4.13.0",
        "ckeditor-plugin/embedbase": "4.13.0",
        "ckeditor-plugin/embedsemantic": "4.13.0",
        "ckeditor-plugin/notification": "4.13.0",
        "ckeditor-plugin/notificationaggregator": "4.13.0",
        "ckeditor-plugin/textmatch": "4.13.0",
        "ckeditor-plugin/wordcount": "1.17.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/bootstrap": "^3.13",
        "drupal/bootstrap_barrio": "^4.17",
        "drupal/chosen": "^2.6",
        "drupal/ckeditor_media_embed": "^1.7",
        "drupal/config_ignore": "^2.1",
        "drupal/config_installer": "^1.8",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/ctools": "^3.0",
        "drupal/date_range_formatter": "^3.1",
        "drupal/default_content": "^1.0@alpha",
        "drupal/devel": "^1.2",
        "drupal/devel_php": "^1.1",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "~2.0",
        "drupal/eva": "^2.0",
        "drupal/field_group": "^1.0",
        "drupal/field_image_link": "1.x-dev",
        "drupal/group": "^1.0@RC",
        "drupal/image_widget_crop": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/legal": "^1.0@RC",
        "drupal/optional_end_date": "^1.0@beta",
        "drupal/pathauto": "^1.3",
        "drupal/r4032login": "^1.1",
        "drupal/shield": "^1.2",
        "drupal/sitemap": "^1.4",
        "drupal/smtp": "^1.0@beta",
        "drupal/social_media_links": "^2.6",
        "drupal/transliterate": "^1.3",
        "drupal/twig_tweak": "^2.1",
        "drupal/twitter_embed": "^1.0@alpha",
        "drupal/views_bootstrap": "^3.1",
        "drupal/views_contextual_filters_or": "^1.1",
        "drush/drush": "^9.0.0",
        "harvesthq/chosen": "^1.8",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/ckeditor/plugins/{$name}": ["vendor:ckeditor-plugin"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "patches": {
            "drupal/legal": {
                "Class attribute should be an array not string": "https://www.drupal.org/files/issues/2018-10-30/legal-set-class-to-array.patch"
            }
        }
    }
}
